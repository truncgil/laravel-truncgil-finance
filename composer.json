{
    "name": "truncgil/finance",
    "description": "A Laravel package to integrate Truncgil Finance API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Truncgil\\Finance\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Truncgil Technology",
            "email": "info@truncgil.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "*",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^10.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Truncgil\\Finance\\FinanceServiceProvider"
            ],
            "aliases": {
                "Finance": "Truncgil\\Finance\\Facades\\Finance"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
